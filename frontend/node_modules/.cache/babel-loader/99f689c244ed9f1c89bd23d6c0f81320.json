AAA;AACA,YAAA,QAAA,CAAA,EAAA,EAAA;AACA,UAAA,KAAA,CAAA,kBAAA,GAAA,KAAA;AACA,iBAAA,QAAA,CAAA,IAAA,EAAA;AACA,SAHA,MAGA;AACA,UAAA,KAAA,CAAA,kBAAA,GAAA,IAAA;AACA,gBAAA,4BAAA,QAAA,CAAA,MAAA;AACA;AACA,OAjBA,EAkBA,IAlBA,CAkBA,UAAA,UAAA,EAAA;AAEA,YAAA,UAAA,CAAA,OAAA,EAAA;AACA,UAAA,KAAA,CAAA,kBAAA,GAAA,KAAA;;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,YAAA,IAAA,EAAA;AAAA,WAAA;AACA,SAHA,MAGA;AACA,UAAA,KAAA,CAAA,kBAAA,GAAA,IAAA;AACA,UAAA,KAAA,CAAA,SAAA,GAAA,UAAA,CAAA,MAAA;AACA;AAEA,OA5BA,WA6BA,UAAA,GAAA;AAAA,eAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA;AAAA,OA7BA;AA8BA;AAhCA,GAlBA;AAoDA,EAAA,OApDA,qBAoDA;AACA,SAAA,IAAA,CAAA,QAAA,GAAA,KAAA,WAAA,CAAA,QAAA;AACA,QAAA,UAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,IAAA;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,gBAAA,UAAA;;AACA,QAAA,UAAA,EAAA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,UAAA;AACA,KAFA,MAEA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,QAAA;AACA;AACA;AA7DA,CAAA","sourcesContent":["<template>\n  <div>\n    <div id=\"register\" class=\"text-center\">\n      <form class=\"form-register\" @submit.prevent=\"register\">\n        <h1 class=\"h3 mb-3 font-weight-normal\">Create {{$route.params.role}} account</h1>\n        <div class=\"alert alert-danger\" role=\"alert\" v-if=\"registrationErrors\">\n          There were problems registering user.\n          <ul>\n            <li v-for='regerror in regErrors' :key=\"regerror\">{{regerror}}</li>\n          </ul>\n          For technical support call Trent Corrill at (513) 555-1212.\n        </div>\n        <label for=\"username\" class=\"sr-only\">Username</label>\n        <b-form-input\n          type=\"text\"\n          id=\"username\"\n          class=\"form-control\"\n          placeholder=\"Username\"\n          v-model=\"user.username\"\n          required\n          autofocus\n        /> \n        <label for=\"password\" class=\"sr-only\">Password</label>\n        <b-form-input\n          type=\"password\"\n          id=\"password\"\n          class=\"form-control\"\n          placeholder=\"Password\"\n          v-model=\"user.password\"\n          required\n        /> \n        <b-form-input\n          type=\"password\"\n          id=\"confirmPassword\"\n          class=\"form-control\"\n          placeholder=\"Confirm Password\"\n          v-model=\"user.confirmPassword\"\n          required\n        />\n        <!-- <router-link :to=\"{ name: 'login' }\">\n          Have an account?\n        </router-link> -->\n        <b-button class=\"btn btn-lg btn-primary btn-block\" type=\"submit\">\n          Create {{$route.params.role}} account\n        </b-button>\n      </form>\n    </div>\n    <div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'register',\n   props: {\n      currentUser: Object\n    },\n  data() {\n    return {\n      user: {\n        username: '',\n        password: '',\n        confirmPassword: '',\n        role: 'parent',\n        familyId: ''\n      },\n      registrationErrors: false,\n      regErrors:  []\n    };\n  },\n  methods: {\n    register() {\n      fetch(`${process.env.VUE_APP_REMOTE_API}/register`, {\n        method: 'POST',\n        headers: {\n          Accept: 'application/json',\n          'Content-Type': 'application/json',\n        },\n        body: JSON.stringify(this.user),\n      })\n\n        .then((response) => {\n          if (response.ok) {\n            this.registrationErrors = false;\n            return (response.json());\n          } else {\n            this.registrationErrors = true; \n            throw 'Registration returned: ' + response.status;\n          }\n        })\n        .then ((parsedData) => {\n              \n              if (parsedData.success) {\n                this.registrationErrors = false;\n                this.$router.push({ path: '/accounts'})\n              } else {\n                this.registrationErrors = true;\n                this.regErrors = parsedData.errors;\n              }\n            \n        })\n        .catch((err) => console.log(err));\n    },\n  },\n    created() {\n    this.user.familyId = this.currentUser.familyId;\n    let rolePassed = this.$route.params.role;\n          console.log(\"rolePassed:\" + rolePassed)\n    if (rolePassed ){\n        this.user.role = rolePassed;\n    } else {\n        this.user.role = 'parent';\n    }\n  }\n}\n</script>\n\n<style scoped>\n  html,body {\n      height: 100%;\n      background-color: #f5f5f5;\n    }\n\n  #register {\n    height: 100%;\n    display: -ms-flexbox;\n    display: flex;\n    -ms-flex-align: center;\n    align-items: center;\n    padding-top: 40px;\n    padding-bottom: 40px;\n    \n    font-family: 'Roboto Condensed', sans-serif;\n  }\n\n  form {\n    text-align: center;\n  }\n\n  .form-register {\n    width: 100%;\n    max-width: 330px;\n    padding: 15px;\n    margin: auto;\n  }\n\n  .form-register .form-control {\n    position: relative;\n    box-sizing: border-box;\n    height: auto;\n    padding: 10px;\n    font-size: 16px;\n  }\n  .form-register .form-control:focus {\n    z-index: 2;\n  }\n  .form-register input[type='text'] {\n    margin-bottom: 10px;\n    border-bottom-right-radius: 0;\n    border-bottom-left-radius: 0;\n  }\n  .form-register input[type='password'] {\n    margin-bottom: 10px;\n    border-top-left-radius: 0;\n    border-top-right-radius: 0;\n  }\n.alert{\n    font-size: 12px;\n    text-align: left;\n  }\n</style>\n"],"sourceRoot":"src/views"}]}