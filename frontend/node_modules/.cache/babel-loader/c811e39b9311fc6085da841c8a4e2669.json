lt {\n  props: {\n      currentUser: Object\n  },\n  data() {\n      return {\n          prize: {\n              familyId: this.currentUser.familyId,\n              prizeName: '',\n              prizeDescription: '',\n              minutesNeeded: '',\n              userRole: '',\n              maxPrizes: '',\n              beginDate: '',\n              endDate: ''\n          }\n      }\n  },\n  methods: {\n      addPrize() {\n          fetch(`${process.env.VUE_APP_REMOTE_API}/api/prize/`, {\n                method: 'POST',\n                headers: {\n                    Accept: 'application/json',\n                    'Content-Type': 'application/json',\n                    Authorization: 'Bearer ' + auth.getToken()\n                    },\n                body: JSON.stringify(this.prize),\n            })\n          .then(() => this.$emit('add-prize'));\n      }\n  }\n}\n</script>\n\n<style scoped>\np {\n    margin: 0;\n}\nform {\n    display: flex;\n    justify-content: space-between;\n}\n#left-side-of-form, #right-side-of-form{\n    width: 45%;\n    margin: 0;\n}\n</style>"],"sourceRoot":"src/components"}]}